# 基本设置
CONFIG_TARGET_mediatek=y
CONFIG_TARGET_mediatek_filogic=y
CONFIG_TARGET_mediatek_mt7986_DEVICE_xiaomi_redmi-router-ax6000=y
CONFIG_TARGET_PROFILE="DEVICE_xiaomi_redmi-router-ax6000"
CONFIG_TARGET_mediatek_mt7986=y
CONFIG_TARGET_SUBTARGET="mt7986"

# 不包含进系统的包
# CONFIG_PACKAGE_dnsmasq is not set
# CONFIG_PACKAGE_luci-app-turboacc is not set
# 软件清单
## luci必须
CONFIG_PACKAGE_luci=y
CONFIG_LUCI_LANG_zh_Hans=y
CONFIG_LUCI_LANG_zh-cn=y

# 主题设置
### CONFIG_PACKAGE_luci-theme-argon=y

## 软件包

CONFIG_PACKAGE_luci-app-openclash=y
CONFIG_PACKAGE_luci-app-wireguard=y
CONFIG_PACKAGE_luci-app-vlmcsd=y
CONFIG_PACKAGE_luci-app-ddns-go=y
CONFIG_PACKAGE_luci-app-mwan3=y
CONFIG_DEFAULT_luci-app-cpufreq=y
CONFIG_PACKAGE_luci-app-syncdial=y
CONFIG_PACKAGE_luci-app-upnp=y
### CONFIG_PACKAGE_luci-app-mosdns=y
### CONFIG_PACKAGE_default-settings=y
