# 基本设置
CONFIG_TARGET_ARCH_PACKAGES="aarch64_cortex-a53"
CONFIG_TARGET_BOARD="mediatek"
CONFIG_TARGET_INIT_CMD="/sbin/init"
CONFIG_TARGET_INIT_ENV=""
CONFIG_TARGET_INIT_PATH="/usr/sbin:/usr/bin:/sbin:/bin"
CONFIG_TARGET_INITRAMFS_COMPRESSION_XZ=y
CONFIG_TARGET_INIT_SUPPRESS_STDERR=y
CONFIG_TARGET_mediatek_filogic_DEVICE_xiaomi_redmi-router-ax6000=y
CONFIG_TARGET_mediatek_filogic=y
CONFIG_TARGET_mediatek=y
CONFIG_TARGET_NAME="aarch64-openwrt-linux-musl"
CONFIG_TARGET_OPTIMIZATION="-Os -pipe -mcpu=cortex-a53"
CONFIG_TARGET_PREINIT_SUPPRESS_STDERR=y
CONFIG_TARGET_PREINIT_TIMEOUT=2
CONFIG_TARGET_PROFILE="DEVICE_xiaomi_redmi-router-ax6000"
CONFIG_TARGET_ROOTFS_DIR=""
CONFIG_TARGET_ROOTFS_INITRAMFS_SEPARATE=y
CONFIG_TARGET_ROOTFS_INITRAMFS=y
CONFIG_TARGET_ROOTFS_PARTSIZE=104
CONFIG_TARGET_ROOTFS_SQUASHFS=y
CONFIG_TARGET_SQUASHFS_BLOCK_SIZE=256
CONFIG_TARGET_SUBTARGET="filogic"
CONFIG_TARGET_SUFFIX="musl"
CONFIG_TARGET_UBIFS_FREE_SPACE_FIXUP=y
CONFIG_TARGET_UBIFS_JOURNAL_SIZE=""
# 显式声明不要编译到系统中
CONFIG_DEFAULT_luci-app-filetransfer=n
CONFIG_PACKAGE_dnsmasq=n
# 软件清单
## luci必须
CONFIG_PACKAGE_uhttpd=y
CONFIG_PACKAGE_uhttpd-mod-ubus=y
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-i18n-base-zh-cn=y
CONFIG_LUCI_LANG_zh_Hans=y
## luci主题,如下都是确认可以使用的主题
CONFIG_PACKAGE_luci-theme-bootstrap=y
CONFIG_PACKAGE_luci-theme-openwrt=y
CONFIG_PACKAGE_luci-theme-openwrt-2020=y
CONFIG_PACKAGE_luci-theme-argon=y

## 软件包
CONFIG_PACKAGE_v2ray-geoip=y
CONFIG_PACKAGE_v2ray-geosite=y
CONFIG_PACKAGE_luci-app-mosdns=y
CONFIG_PACKAGE_luci-app-openclash=y
CONFIG_PACKAGE_luci-app-passwall2=y
CONFIG_PACKAGE_luci-app-wireguard=y
CONFIG_PACKAGE_luci-app-autoreboot=y
CONFIG_PACKAGE_luci-app-vlmcsd=y
CONFIG_PACKAGE_luci-app-fakemesh=y
CONFIG_PACKAGE_luci-app-vssr=y
#CONFIG_PACKAGE_luci-app-adguardhome=y

## IPV6
CONFIG_KERNEL_IPV6=n
CONFIG_KERNEL_IPV6_MULTIPLE_TABLES=n
CONFIG_KERNEL_IPV6_SUBTREES=n
CONFIG_KERNEL_IPV6_MROUTE=n
CONFIG_KERNEL_IPV6_SEG6_LWTUNNEL=n
CONFIG_IPV6=n
CONFIG_BUSYBOX_DEFAULT_FEATURE_IPV6=n
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=n
CONFIG_PACKAGE_luci-proto-ipv6=n
